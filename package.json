{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --base '/themes/custom/middlebury_theme/'",
    "build:icons": "NODE_ENV=production gulp icons",
    "postbuild": "npm run test",
    "preview": "vite preview",
    "test": "npm run test:bundlesize",
    "lint": "npm run lint:styles",
    "lint:styles": "stylelint src/scss/*",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{js,ts,scss}\"",
    "test:bundlesize": "bundlesize"
  },
  "devDependencies": {
    "@types/anchor-js": "^5.0.3",
    "@types/clamp-js-main": "^0.11.5",
    "@types/headroom.js": "^0.12.4",
    "@types/lodash": "^4.17.20",
    "@types/lozad": "^1.16.4",
    "@types/micromodal": "^0.3.5",
    "@types/node": "^24.3.0",
    "@types/pikaday": "^1.7.9",
    "@vheemstra/vite-plugin-imagemin": "^2.2.0",
    "@vituum/vite-plugin-twig": "^1.1.0",
    "autoprefixer": "^10.4.21",
    "bundlesize": "^0.18.2",
    "cssnano": "^7.1.0",
    "glob": "^11.0.3",
    "gulp": "^5.0.1",
    "gulp-rename": "^2.1.0",
    "gulp-svg-sprite": "^2.0.3",
    "gulp-svgo": "^2.2.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.6.2",
    "rollup-plugin-license": "^3.6.0",
    "sass-embedded": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-xo-scss": "^1.0.1",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3",
    "vite": "^7.1.3",
    "vituum": "^1.2.0"
  },
  "dependencies": {
    "anchor-js": "^5.0.0",
    "animejs": "^4.1.3",
    "chart.js": "^4.5.0",
    "focus-within-polyfill": "^5.2.1",
    "headroom.js": "^0.12.0",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "lozad": "^1.16.0",
    "mdn-polyfills": "^5.20.0",
    "micromodal": "^0.6.1",
    "normalize.css": "^8.0.1",
    "object-fit-images": "^3.2.4",
    "panelsnap": "github:poojagunturu96/panelsnap",
    "pikaday": "^1.8.2",
    "preact": "^10.27.1",
    "promise-polyfill": "^8.3.0",
    "stickybits": "^3.7.11",
    "superclamp": "^0.2.3",
    "swiper": "^11.2.8"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "55 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/js/main.bundle.js",
      "maxSize": "35 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/js/chunks/preload-helper.js",
      "maxSize": "25 kB",
      "compression": "gzip"
    }
  ]
}
