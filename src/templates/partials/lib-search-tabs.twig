{% embed 'tabs.twig' with {
  light: true,
  tablist: [
    { label: 'All', id: 'midd-lib-all' },
    { label: 'MIDCAT', id: 'midd-lib-midcat' },
    { label: 'Guides', id: 'midd-lib-guides' },
    { label: 'Journals', id: 'midd-lib-journals' },
    { label: 'Database', id: 'midd-lib-database' },
    { label: 'Reserves', id: 'midd-lib-reserves' },
    { label: 'Videos', id: 'midd-lib-videos' },
