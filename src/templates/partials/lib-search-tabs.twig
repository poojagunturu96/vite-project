    {% embed 'tab.twig' with { id: 'midd-lib-journals' } %}
      {% block tabcontent %}

        <p class="mb-5 f4">Find print and electronic journals, magazines, and newspapers.</p>

        <form action="http://XU8KT9SN3C.search.serialssolutions.com" method="get">
          <input name="V" type="hidden" value="1.0">
          <input name="N" type="hidden" value="100">
          <input name="L" type="hidden" value="XU8KT9SN3C">

          <div class="row">
            <div class="col-md-6 col-lg-5">
              <div class="form-group">

                <label for="lib-journals-title-begins">Find journals by title or ISSN</label>
                <select name="S" id="lib-journals-title-begins" class="form-control">
                  <option value="AC_T_B">Title begins with</option>
                  <option value="AC_T_M">Title equal</option>
                  <option value="T_W_A">Title contains all words</option>
                  <option value="I_M">ISSN equals</option>
                </select>
              </div>
            </div>

            <div class="col-md-6 col-lg-7">
              <div class="form-group">
                <label for="SS_CFocusTag">Find a Journal</label>
                <div class="input-group">
                  <input id="SS_CFocusTag" name="C" class="form-control">
                  <input type="submit" value="Find" class="button button--primary">
                </div>
              </div>
            </div>
          </div>
        </form>

        <div class="form-group">
          <h3 class="label">Browse journals by title</h3>

          <ul class="d-flex flex-wrap">
            <li class="mr-3">
              <a href="http://XU8KT9SN3C.search.serialssolutions.com/?V=1.0&L=XU8KT9SN3C&N=100&S=T_AZ&C=0" class="link-underline">0-9</a>
            </li>
            {% for i in 1..26 %}
              <li class="mr-3">
                {% set letter = range('A','Z')[loop.index - 1] %}
                <a href="http://XU8KT9SN3C.search.serialssolutions.com/?V=1.0&L=XU8KT9SN3C&N=100&S=T_AZ&C={{letter}}" class="link-underline">
