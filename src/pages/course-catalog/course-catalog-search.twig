                  <input type="checkbox" class="checkbox__input" id="checkbox-level-item-{{loop.index}}" value="topic/level/{{item.value}}">
                  <label for="checkbox-level-item-{{loop.index}}" id="checkbox-level-label-{{loop.index}}" class="checkbox__label">{{item.label}}</label>
                </div>
              {% endfor %}
            </div>
          </fieldset>
        </div>
      </div>
    </div>

    <div class="col-12 col-xl-4 form-group">
      <input type="submit" value="Search" class="button button--primary d-block d-md-inline-block">
    </div>

  </div>
</form>
