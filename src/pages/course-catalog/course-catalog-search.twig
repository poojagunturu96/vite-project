                  <label for="checkbox-mod-item-{{loop.index}}" id="checkbox-mod-label-{{loop.index}}" class="checkbox__label">{{item.label}}</label>
                </div>
              {% endfor %}
            </div>
          </fieldset>

          <fieldset class="col-md-6 form-group">
            <legend class="label">
              <p class="f2">Level</p>
              <p class="f1">(Leave all blank for no preference)</p>
            </legend>
            <div class="checkbox-group">
              {% for item in catalog_search_items.level %}
